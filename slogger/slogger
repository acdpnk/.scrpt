MAJOR_VERSION = 2
MINOR_VERSION = 1
BUILD_NUMBER = 14

init_env = ENV['SLOGGER_NO_INITIALIZE'].to_s
ENV['SLOGGER_NO_INITIALIZE'] = "false"

require File.expand_path('../slogger',__FILE__)

ENV['SLOGGER_NO_INITIALIZE'] = init_env
